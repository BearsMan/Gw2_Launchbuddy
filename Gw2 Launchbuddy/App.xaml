<Application x:Class="Gw2_Launchbuddy.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Gw2_Launchbuddy"
             StartupUri="MainWindow.xaml">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary x:Name="lb_colorpalette" Source="ColorPalettes/Launchbuddy_dark_colorpalette.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Launchbuddy_theme.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>